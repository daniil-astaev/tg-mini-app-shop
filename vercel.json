{
  "version": 2,
  "builds": [
    {"src": "api/bot.js", "use": "@vercel/node"},
    {"src": "package.json", "use": "@vercel/static-build", "config": {"distDir": "dist"}}
  ],
  "routes": [
    {"src": "/api/bot", "dest": "/api/bot.js"},
    {"src": "/(.*)", "dest": "/dist/$1"}
  ],
  "env": {
    "BOT_TOKEN": "",
    "WEB_APP_URL": ""
  }
}